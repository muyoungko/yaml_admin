{
  "name": "yaml-admin",
  "version": "0.0.1",
  "private": true,
  "description": "yaml admin monorepo (workspaces)",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "export NODE_ENV=development && node_modules/.bin/concurrently \"npm run dev:api\" \"npm run dev:front\"",
    "dev:api": "npm run dev -w yaml-admin-api",
    "dev:front": "npm run dev -w yaml-admin-front",
    "build:front": "npm run build -w yaml-admin-front",
    "publish:front": "npm publish -w yaml-admin-front --access public",
    "publish:api": "npm publish -w yaml-admin-api --access public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/muyoungko/yaml_admin.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/muyoungko/yaml_admin/issues"
  },
  "homepage": "https://github.com/muyoungko/yaml_admin#readme",
  "dependencies": {
    "concurrently": "^9.2.0"
  }
}
